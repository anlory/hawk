
rust_binary {
    name: "hawk",
    crate_name: "hawk",
    srcs: [
        "src/hawk.rs",
    ],
    rustlibs: [
        "liblogger",
        "liblog_rust",
        "com.anlory.hawk-rust",
    ],
    prefer_rlib: true,
}

rust_binary {
    name: "hawk_client",
    crate_name: "hawk_client",
    srcs: [
        "src/client.rs",
    ],
    rustlibs: [
        "liblogger",
        "liblog_rust",
        "com.anlory.hawk-rust",
    ],
    prefer_rlib: true,
}